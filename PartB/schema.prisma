datasource db {
  provider = "postgresql"
  url      = "postgresql://johndoe:randompassword@localhost:5432/mydb?schema=public"
}

model User {
  id      Int      @id @default(autoincrement())
  email   String   @unique
  name    String?
  posts   Post[]
  profile Profile?
}

model Product {
  id      Int      @id @default(autoincrement())
  name    String?
  desc    String?
}

